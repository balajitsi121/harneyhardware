{% include "object-current-params" %}

{% assign current_params_qty = false %}

{% for key in current_param_keys %}
  {% assign key_decoded = key | url_decode %}

  {% if key_decoded == "q[qty]" %}
    {% assign current_params_qty = current_param_values[forloop.index0] | downcase | url_decode | split: "," %}
    {% break %}
  {% endif %}
{% endfor %}
