/*============================================================================
  #Shopify Header Search
==============================================================================*/
div[ss-autocomplete]{
  top:37px !important;
  @include breakpoint(null, null, $xlarge - 1) {
    padding: 0 50px 0 50px;
    left: -50px;
    -webkit-box-sizing: content-box;
    box-sizing: content-box;
    width:100%;
    background-color: #FFF;
  }
  @include breakpoint(null, null, $medium - 118) {
    padding: 0 20px;
    left: -20px;
  }
  div.ss-ac-wrapper{
    padding-left:170px;
    border-radius: 5px;
    border-top-left-radius: 0;
    border-color:#6d6d6d !important;
    box-shadow: none !important;
    -webkit-box-shadow: none !important;
    @include breakpoint(null, null, $xlarge - 1) {
      padding-left:0;
      border-radius: 0;
      border:0;
    }
    #ss-ac-terms{
      margin: 0 0 0 -170px;
      width: 170px;
      @include breakpoint(null, null, $xlarge - 1) {
        margin: 0;
        width: 100%;
        position: relative;
        padding: 8.8px 0;
      }
      @include breakpoint(null, null, $medium) {
        border:0;
      }
      &:after{
        @include breakpoint(null, null, $xlarge - 1) {
            content: " ";
            position: absolute;
            width: 50px;
            height: 100%;
            background-color: $color_accent_light;
            top: 0;
            width: 100%;
            left: -50px;
            padding-right: 100px;
            box-sizing: content-box;
            z-index: -1;
        }
        @include breakpoint(null, null, $medium - 118) {
          left: -20px;
          padding-right: 40px;
        }
      }
        ul{
          @include breakpoint(null, null, $xlarge - 1) {
            @include flexbox;
            @include flex-wrap(wrap);
            gap:10px 30px;
          }
          li{
            @include breakpoint(null, null, $small) {
              width:auto;
            }
            &.active{
              background-color:#eeeeee;
              @include breakpoint(null, null, $xlarge - 1) {
                background-color:transparent;
              }
              a{
                color: $color_default;
                text-decoration:underline;
              }
            }
            a{
              @extend %font-primary;
              font-size: 14px;
              color: $color_default;
              padding-top:16px;
              padding-bottom:16px;
              padding-left: 10px;
              padding-right: 10px;
              line-height: 1.4;
              text-align: left;
              @include breakpoint(null, null, $xlarge - 1) {
                padding: 5px 0;
                font-size: 12px;
              }
            }
          }
        }
      }
      #ss-ac-content{
        border-radius:5px;
        @include breakpoint(null, null, $xlarge - 1) {
          border-radius:0;
          width:100%;
          padding-left:0;
          padding-right:0;
        }
        #ss-ac-facets{
          @include breakpoint(null, null, $xlarge - 1) {
            display:none;
          }
          .facet-container{
            &:last-child{
              ul.ng-scope{
                border-bottom: 1px solid #eeeeee;
                padding-bottom:20px;
              }  
            }
            h4{
              @extend %font-primary;
              margin-bottom:10px;
              line-height: 1.05;
              letter-spacing: 0.7px;
              font-size: 14px;
            }
            .ng-scope.list{
              &:last-child{
                ul.ng-scope{
                  margin-bottom:0;
                }
              }
            }
            ul.ng-scope{
              margin-bottom:20px;
              border-color:#eeeeee;
              li{
                &:last-child{
                  a{
                    margin-bottom:0;  
                  }
                }
                a{
                  @extend %font-primary;
                  font-size: 12px;
                  line-height: 1.4;
                  color:$color-default;
                  margin-bottom:10px;
                }
              }
            }
          }
        }
        #ss-ac-results{
          @include breakpoint(null, null, $xlarge - 1) {
            overflow:inherit;
            margin-top:57px;
          }
          @include breakpoint(null, null, $xlarge - 79) {
            margin-top:10px;
          }
          @include breakpoint(null, null, $large) {
            margin-top:20px;
          }
          @include breakpoint(null, null, $medium) {
            margin-top:5px;
          }
          h4{
            @extend %font-secondary;
            font-size:20px;
            line-height: 1.3;
            color:$color-default;
            text-transform: none;
            .ng-binding{
              color:$color-default;
              text-transform: capitalize;
            }
            @include breakpoint(null, null, $xlarge - 1) {
              margin-bottom:24px;
            }
            @include breakpoint(null, null, $medium) {
              display:block;
              font-size:18px;
              margin-bottom:10px;
            }
          }
          .item-results{
            @include breakpoint(null, null, $xlarge - 1) {
              margin: 0;
              display: flex;
              flex-wrap: wrap;
              gap: 40px 25px;
              justify-content: space-between;
            }
            @include breakpoint(null, null, $medium) {
              gap: 10px;
            }
            li.item{
              padding: 0 6px !important;
              @include breakpoint(null, null, $xlarge - 1) {
                width: calc(50% - 25px);
                padding: 0 !important;
                margin-bottom:0;
              }
              @include breakpoint(null, null, $large) {
                display: inline-block !important;
              }
              @include breakpoint(null, null, $medium - 118) {
                width: calc(50% - 16px);
              }
              .item-image{
                height:150px;
                margin-bottom:0;
                @include breakpoint(null, null, $xlarge - 1) {
                  height:342px;
                  max-width:342px;
                  width: 100%;
                  margin: 0 auto;
                }
                @include breakpoint(null, null, $medium - 118) {
                  height:150px;
                  max-width:150px;
                }
                .image-wrapper{
                  img{
                    @include breakpoint(null, null, $xlarge - 1) {
                      max-width: 100%;
                      max-height: 100%;
                      width: 100%;
                      height: 100%;
                      object-fit: none;
                    }
                    @include breakpoint(null, null, $medium - 118) {
                      object-fit: contain;
                    }
                  }
                }
              }
              .item-details{
                @extend %font-primary;
                text-align:center;
                line-height: 1.4;
                @include breakpoint(null, null, $medium - 118) {
                  max-width:150px;
                  margin: 0 auto;
                  padding: 0 5px;
                }
                .item-name{
                  @extend %font-primary;
                  margin-bottom:3px;
                  color:$color-default;
                  &:active, &:hover{
                    color:$color-default;
                    text-decoration:underline;
                  }
                }
                .item-price{
                  @extend %font-primary;
                  font-weight:bold;
                }
              }
            }
          }
        }
      }
    }
}
