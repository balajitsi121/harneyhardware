@import url('https://fonts.googleapis.com/css?family=Droid+Sans:400,700');

/*============================================================================
  #Variables
==============================================================================*/

$color-default: {{ settings.color_default }};
$color-primary: {{ settings.color_primary }};
$color-secondary: {{ settings.color_secondary }};
$color-accent: {{ settings.color_accent }};
$color-accent-light: {{ settings.color_accent_light }};

$color-detail: #e0e0e0;

$colors: (default,$color-default) (primary,$color-primary) (secondary,$color-secondary) (accent,$color-accent) (accent-light, $color-accent-light) (inverted,#FFFFFF) (detail,$color-detail) (dark,#000000);

$xsmall: 320px;
$small: 480px;
$medium: 768px;
$large: 1024px;
$xlarge: 1280px;
$xxlarge: 1364px;

$pre-small: $small - 1px;
$post-small: $small + 1px;
$pre-medium: $medium - 1px;
$post-medium: $medium + 1px;
$pre-large: $large - 1px;
$post-large: $large + 1px;
$pre-xlarge: $xlarge - 1px;
$post-xlarge: $xlarge + 1px;
$pre-xxlarge: $xxlarge - 1px;

$default-breakpoints: (
  "small" "(max-width: #{$small})",
  "medium" "(min-width: #{$small + 1px})",
  "medium-down" "(max-width: #{$medium})",
  "medium-only" "(min-width: #{$post-small}) and (max-width: #{$medium})",
  "large" "(min-width: #{$medium + 1px})",
  "large-down" "(max-width: #{$large})",
  "xlarge" "(min-width: #{$xlarge})",
  "xlarge-down" "(max-width: #{$pre-xlarge})",
  'xxlarge' '(min-width: #{$xxlarge})'
);

/*============================================================================
  #Modular scales (h/t Bourbon)
==============================================================================*/

$golden:           1.618;
$minor-second:     1.067;
$major-second:     1.125;
$minor-third:      1.2;
$major-third:      1.25;
$perfect-fourth:   1.333;
$augmented-fourth: 1.414;
$perfect-fifth:    1.5;
$minor-sixth:      1.6;
$major-sixth:      1.667;
$minor-seventh:    1.778;
$major-seventh:    1.875;
$octave:           2;
$major-tenth:      2.5;
$major-eleventh:   2.667;
$major-twelfth:    3;
$double-octave:    4;

@function modular-scale($increment, $ratio: $minor-third, $value: 1rem) {
  $v1: nth($value, 1);
  $v2: nth($value, length($value));
  $value: $v1;

  // scale $v2 to just above $v1
  @while $v2 > $v1 {
    $v2: ($v2 / $ratio); // will be off-by-1
  }
  @while $v2 < $v1 {
    $v2: ($v2 * $ratio); // will fix off-by-1
  }

  // check AFTER scaling $v2 to prevent double-counting corner-case
  $double-stranded: $v2 > $v1;

  @if $increment > 0 {
    @for $i from 1 through $increment {
      @if $double-stranded and ($v1 * $ratio) > $v2 {
        $value: $v2;
        $v2: ($v2 * $ratio);
      } @else {
        $v1: ($v1 * $ratio);
        $value: $v1;
      }
    }
  }

  @if $increment < 0 {
    // adjust $v2 to just below $v1
    @if $double-stranded {
      $v2: ($v2 / $ratio);
    }

    @for $i from $increment through -1 {
      @if $double-stranded and ($v1 / $ratio) < $v2 {
        $value: $v2;
        $v2: ($v2 / $ratio);
      } @else {
        $v1: ($v1 / $ratio);
        $value: $v1;
      }
    }
  }

  @return $value;
}

/*============================================================================
  #Modular scale functions for type and layout
==============================================================================*/

$box-scale: $perfect-fourth;
$text-scale: $major-second;

@function box-scale($increment) {
  @return modular-scale($increment, $box-scale);
}

@function text-scale($increment) {
  @return modular-scale($increment, $text-scale);
}

/*============================================================================
  #Mixins
==============================================================================*/

@mixin vendor-prefix($property, $value) {
  -webkit-#{$property}: unquote($value);
  -ms-#{$property}: unquote($value);
  -moz-#{$property}: unquote($value);
  -o-#{$property}: unquote($value);
  #{$property}: unquote($value);
}

@mixin keyframes($animation-name) {
  @-webkit-keyframes $animation-name {
    @content;
  }
  @-moz-keyframes $animation-name {
    @content;
  }
  @keyframes $animation-name {
    @content;
  }
}

@function get-media-query($keyword, $breakpoints: $default-breakpoints) {
  @each $breakpoint in $breakpoints {
    $breakpoint-key: nth($breakpoint, 1);
    $media-query: nth($breakpoint, 2);

    @if $breakpoint-key == $keyword {
      @return $media-query;
    }
  }
}

@mixin breakpoint($keyword: null, $min-width: null, $max-width: null) {
  @if $keyword {
    $media-query: get-media-query($keyword);

    @media screen and #{$media-query} {
      @content;
    }
  } @else if $min-width and $max-width {
    @media screen and (min-width: $min-width) and (max-width: $max-width) {
      @content;
    }
  } @else if $min-width {
    @media screen and (min-width: $min-width) {
      @content;
    }
  } @else if $max-width {
    @media screen and (max-width: $max-width) {
      @content;
    }
  }
}

/*============================================================================
  #Reset
==============================================================================*/

@mixin reset() {
  //
  // Layout
  //

  *, :before, :after {
    @include vendor-prefix(box-sizing, border-box);
  }

  body, h1, h2, h3, h4, h5, h6, p, blockquote, figure, ol, ul {
    margin: 0;
    padding: 0;
  }

  img {
    height: auto;
    border: 0;
  }

  li {
    display: block;
  }

  //
  // Type
  //

  html {
    -ms-text-size-adjust: 100%;
    -webkit-text-size-adjust: 100%;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
  }

  h1, h2, h3, h4, h5, h6 {
    font-size: inherit;
    // Optimize legibility on headers only to
    // prevent performance issues from applying
    // it to large portions of text.
    text-rendering: optimizeLegibility;
    font-weight: normal;
  }

  a {
    background-color: transparent;
    text-decoration: none;
    color: inherit;

    &:active,
    &:hover {
      outline: 0;
    }
  }

  strong {
    font-weight: bold;
  }
  em {
    font-style: italic;
  }

  sup {
    position: relative;
    top: -(box-scale(-6));
    vertical-align: top;
    font-size: text-scale(-2);
  }

  //
  // Forms
  //

  input,
  textarea,
  select {
    display: block;
    width: 100%;

    &[disabled] {
      cursor: default;
    }

    border-radius: 0px;
    -webkit-border-radius: 0px;
    -webkit-appearance: none;
  }

  .border-radius, .hover-border-radius:hover {
    @include vendor-prefix(border-radius, 5px);
  }
  .border-radius-top-left, .hover-border-radius-top-left:hover {
    @include vendor-prefix(border-top-left-radius, 5px);
  }

  .border-radius-top-right, .hover-border-radius-top-right:hover {
    @include vendor-prefix(border-top-right-radius, 5px);
  }

  .border-radius-bottom-left, .hover-border-radius-bottom-left:hover {
    @include vendor-prefix(border-bottom-left-radius, 5px);
  }

  .border-radius-bottom-right, .hover-border-radius-bottom-right:hover {
    @include vendor-prefix(border-bottom-right-radius, 5px);
  }

  label {
    display: inline-block;
  }

  label > * {
    pointer-events: none;
  }

  input::-moz-focus-inner {
    border: 0;
    padding: 0;
  }

  input[type=radio] {
    @include vendor-prefix(appearance, radio);
  }

  input[type=checkbox] {
    @include vendor-prefix(appearance, checkbox);
  }

  input[type="checkbox"], input[type="radio"] {
    display: inline;
    margin-bottom: 0;

    // prevent padding set on the input element
    // from applying to radio buttons or checkboxes
    // on iOS Safari
    padding: 0 !important;
  }

  input[type="search"] {
    @include vendor-prefix(appearance, textfield);
    border-radius: 5%;
    -webkit-border-radius: 5px;
    -webkit-appearance: none;

    &::-webkit-search-decoration {
      -webkit-appearance: none;
    }
  }

  input[type="submit"], button {
    cursor: pointer;
  }

  input[type="image"] {
    border: none;
  }
}

@include reset();

/*============================================================================
  #Base
==============================================================================*/

@mixin base() {
  html, body {
    background-color: #FFFFFF;
  }

  [data-src]:not(iframe) {
    opacity: 0;

    &.loaded {
      @include vendor-prefix(animation, fade-in 250ms both);
    }
  }

  img:not([src]) {
    height: 0;
  }
}

@include base();

/*============================================================================
  #Animation
==============================================================================*/

@mixin animation() {
  .force-hardware-acceleration {
    -webkit-transform: translate3d(0,0,0);
    -webkit-backface-visibility: hidden;
  }

  @include keyframes(fade-in) {
    0% {
        opacity: 0;
    }
    100% {
        opacity: 1;
    }
  }

  @include keyframes(fade-out) {
    0% {
      opacity: 1;
    }
    100% {
      opacity: 0;
    }
  }

  .fade-in {
    @include vendor-prefix(animation, fade-in 250ms both);
  }
  .fade-out {
    @include vendor-prefix(animation, fade-in 250ms both);
  }
}

@include animation();

/*============================================================================
  #Layout
==============================================================================*/

@mixin layout() {
  %clearfix, .clearfix {
    &:after {
      content: " ";
      display: block;
      clear: both;
    }
  }

  .box-center {
    margin-right: auto;
    margin-left: auto;
  }
  .medium-only--box-center {
    @include breakpoint(null, $medium, $pre-large) {
      margin-right: auto;
      margin-left: auto;
    }
  }
  .box-placeholder {
    background-color: #dddddd;
  }

  .block {
    display: block;
  }
  .inline-block {
    display: inline-block;
  }
  .inline-block-middle {
    display: inline-block;
    vertical-align: middle;
  }

  .max-one-whole {
    max-width: 100%;
  }

  .auto-width {
    width: auto;
  }

  .invisible {
    border: 0;
    clip: rect(0 0 0 0);
    height: 1px;
    margin: -1px;
    overflow: hidden;
    padding: 0;
    position: absolute;
    width: 1px;
  }

  .overflow-hidden {
    overflow: hidden;
  }

  // .nowrap {
  //   white-space: nowrap;
  // }

  .scroll-horizontal {
    overflow-y: hidden;
    overflow-x: auto;
    -webkit-overflow-scrolling: touch;
  }

  %container, .container {
    padding-right: box-scale(1);
    padding-left: box-scale(1);
  }

  %theme-width, .theme-width {
    max-width: 1920px;
  }
  .theme-width-center {
    @extend %theme-width;
    margin-right: auto;
    margin-left: auto;
  }
  .theme-container {
    @extend %container;
    @extend %theme-width;
    margin-right: auto;
    margin-left: auto;
  }
  main{
    .theme-container.cover-container {
      padding:60px 100px;
      @include breakpoint(null, null, $xxlarge + 76) {
        padding:60px;
      }
      @include breakpoint(null, null, $medium + 66) {
        padding:50px;
      }
      @include breakpoint(null, null, $medium + 66) {
        padding:40px 20px;
      }
    }
  }

  .customer-form-container {
    @extend %container;
    margin-right: auto;
    margin-left: auto;
    max-width: 943px;
  }

  .image-container {
    display: block;
    font-size: 0;
    line-height: 1;

    img {
      width: 99.99999%;
    }
  }

  .video-container {
    position: relative;

    iframe {
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
    }
  }

  @each $value in baseline, middle, top, bottom {
    .align-#{$value} {
      vertical-align: $value;
    }
  }
}

@include layout();

/*============================================================================
  #Typography
==============================================================================*/

@mixin typography() {
  %font-primary, .font-primary {
    font-family: Arial,Helvetica Neue,Helvetica,sans-serif;
  }

  %font-secondary, .font-secondary {
    font-family: 'Droid Sans', sans-serif;
  }

  html {
    font-size: 14px;
  }

  body, input, textarea, select, button {
    @extend %font-primary;
    font-size: text-scale(0);
    color: $color-default;
  }

  h1, h2, h3, h4, h5, h6, p, ul, ol, blockquote, hr, input, textarea, select, button {
    @extend %font-secondary;
    margin-bottom: text-scale(0);
  }

  p, ul, ol, blockquote, label {
    line-height: text-scale(2);
  }

  a {
    @include vendor-prefix(transition, .2s);
  }

  hr {
    margin-top: 0;
  }

  small {
    font-size: text-scale(-1);
  }

  .cursor-pointer {
    cursor: pointer;
  }
  .cursor-default {
    cursor: default;
  }



  %font-thin, .font-thin, %font-lightest, .font-lightest {
    font-weight: 100;
  }
  %font-light, .font-light {
    font-weight: 300;
  }
  %font-regular, .font-regular {
    font-weight: 400;
  }
  %font-medium, .font-medium {
    font-weight: 500;
  }
  %font-bold, .font-bold {
    font-weight: 700;
  }
  %font-bolder, .font-bolder {
    font-weight: 800;
  }
  %font-black, .font-black, %font-boldest, .font-boldest {
    font-weight: 900;
  }

  .text-hidden {
    visibility: hidden;
  }
  .text-justify {
    text-align: justify;
  }
  .text-caps {
    text-transform: uppercase;
  }
  .text-capitalize {
    text-transform: capitalize;
  }
  .text-regular-case {
    text-transform: none;
  }
  .text-underline {
    text-decoration: underline;
  }
  .hover-text-underline:hover {
    text-decoration: underline !important;
  }
  .text-line-through {
    text-decoration: line-through;
  }
  .text-decoration-none {
    text-decoration: none;
  }

  %font-regular, .font-regular {
    font-weight: 400;
  }
  %font-bold, .font-bold {
    font-weight: 700;
  }
}

@include typography();


/*============================================================================
  #Forms
==============================================================================*/

@mixin forms() {
  .input {
    @include vendor-prefix(appearance, none);
    padding: box-scale(-3) box-scale(-1);
    border: 0.5px solid #d0d0d0;
    @extend %font-primary;
    &:focus-visible{
      outline: inherit;
    }
    &.ss-ac-visible{
      border-color:#6d6d6d;
    }
  }

  label + .input {
    margin-top: box-scale(-3);
  }

  .select {
    @include vendor-prefix(appearance, none);
    background: none;
    background-image: url('data:image/png;base64,');
    background-repeat: no-repeat;
    background-size: 11px 8px;
    background-position: calc(100% - 0.75019rem) center;
    padding: box-scale(-3) 30px box-scale(-3) box-scale(-1);
  }
}

@include forms();

/*============================================================================
  #Buttons
==============================================================================*/

@mixin buttons() {
  .button-reset {
    border: none;
    background: none;
    margin-bottom: 0;
    color: inherit;
    padding: 0;
  }

  @each $color in $colors {
    $color-key: nth($color, 1);
    $color-hex: nth($color, 2);

    .button-#{$color-key} {
      border: none;
      padding: box-scale(-1) box-scale(3);
      display: inline-block;
      color: #ffffff;
      background-color: $color-hex;
    }

    .button-#{$color-key}-filled {
      border: 2px solid $color-hex;
      display: inline-block;
      background-color: $color-hex;
      text-transform: uppercase;
      font-size: text-scale(-1);
      @extend %font-secondary;
      @extend %font-bold;
      @if $color-key == "inverted" {
        color: $color-primary;
        &:hover {
          border-color: $color-primary;
        }
      } @else {
        color: #ffffff !important;
        &:hover {
          background-color: white;
          color: $color-primary !important;
          border-color: white;
        }
      }

      &.tall {
        padding-top: box-scale(2);
        padding-bottom: box-scale(2);
      }
    }

    .button-#{$color-key}-bordered {
      border: 2px solid $color-hex;
      display: inline-block;
      background-color: #ffffff;
      text-transform: uppercase;
      font-size: text-scale(-1);
      @extend %font-secondary;
      @extend %font-bold;
      color: $color-hex;

      &:hover, &.in-cart {
        background-color: $color-hex;
        color: #ffffff !important;
      }

      &.tall {
        padding-top: box-scale(2);
        padding-bottom: box-scale(2);
      }
    }
  }
}

@include buttons();

/*============================================================================
  #RTE
==============================================================================*/

@mixin rte() {
  %list-default, .list-default {
    > li {
      display: list-item;
      list-style-position: inside;
    }
  }

  %text-links, .text-links {
    a {
      color: $color-primary;
      // text-decoration: underline;
    }
    a:hover {
      color: darken($color-primary, 10%);
    }
  }

  %table, .table, table {
    &, table {
      width: 100%;
      border-collapse: collapse;
      border-spacing: 0;
      margin-bottom: text-scale(0);

      &.bordered {
        border-spacing: 0px;
        thead, tfoot {
          background-color: #f3f3f3;
        }
        th {
          font-weight: bold;
          background-color: #f3f3f3;
        }
        th, td {
          text-align: left;
          padding: 12px;
          padding-top: 30px !important;
          padding-bottom: 30px !important;
          border-bottom: 1px solid #f3f3f3;

          @include breakpoint(null, $xsmall, $pre-medium) {
            padding: 4px;
            font-size: 13px;
          }
        }

      }
    }
    th {
      text-align: left;
    }
    tr:first-child {
      // border-bottom: 1px solid lighten($color-default, 50%);

      td {
        // font-weight: bold;
        // padding-bottom: text-scale(0);
      }
    }
    tr:not(:first-child) {
      td {
        padding-top: text-scale(0);
        padding-bottom: text-scale(0);
      }
    }
  }

  .rte {
    @extend %text-links;
    @extend %table;

    a {
      font-weight: 700;
    }

    p {
      font-size: text-scale(0);
      line-height: text-scale(5);
      font-weight: 400;
    }

    p {
      margin-bottom: text-scale(6);
    }

