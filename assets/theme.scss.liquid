            }
          }
        }
      }
    }

    @include breakpoint(null, null, '1460px') {
      &:nth-child(n+6) {
        &:hover {
          .js-dropdown {
            left: auto;
            right:0;
            .child-menu-wrapper{
              order:2
            }
          }
        }
      }
    }


    .icon-carat-default-small {
      -webkit-transition: none !important;
      -moz-transition: none !important;
      -o-transition: none !important;
      transition: none !important;

      @include vendor-prefix(transition, none !important);
    }

    &:before {
      content: "";
      position: absolute;
      top: 0;
      left: 0;
      width:100%;
      height:100%;
      padding-bottom: 0;
    }
    &:hover {
      // border-top: 1px solid $color-default;
      // border-bottom: 1px solid $color-default;
      // border-left: 1px solid $color-default;
      // border-right: 1px solid $color-default;

      &:before {
        content: "";
        position: absolute;
        border-left: 0;
        // border-top: 1px solid #d7d7d7;
        top: 0;
        left: 0px;
        width: 100%;
        height: calc(100% + 27px);

        @include breakpoint(null, $large, $pre-xlarge) {
          left: -10px;
          width: calc(100% + 36px);
        }
      }

      &:after {
        content: "";
        position: absolute;
        border-right: 0;
        border-bottom: 0;
        bottom: 0px;
        right: -0px;
        width: 100%;
        height: 100%;
        z-index: 11;
        padding-bottom: 0;

        @include breakpoint(null, $large, $pre-xlarge) {
          right: -26px;
          width: calc(100% + 35px);
        }
      }
    }
  }

  .js-dropdown-trigger {
    &:hover {
      border-top: 0;
      color: $color-default;
      position: relative;
      top: 0;
      > a {
        color: $color-default;
      }
      .js-dropdown {
        display: block;
      }


    }

    > a {
      &:after {
        font-family: FontAwesome;
        font-size: 11px;
        position: relative;
        left: 10px;
        top: -1px;
      }
    }
  }

  .theme-dropdown {
    z-index: 9999;
    &:before {
      content: '';
      border-left: 1px solid #d7d7d7;
      position: absolute;
      top: 0;
      left: 0;
      width: 1px;
      height: 1px;
    }
    &:after {
      content: '';
      border-bottom: 0;
      position: absolute;
      bottom: 0;
      width: calc(100% + 2px);
      left: -1px;
    }
  }

  .js-dropdown {
    position: absolute;
    z-index: 11;
    background-color: white;
    margin-top: 23px;
    left: 0px; // no IDEA why this works but it does.......
    display: none;
    width: 277px;
    padding-top: 17px;
    padding-left: 16px;
    padding-right: 16px;
    padding-bottom: 30px;
