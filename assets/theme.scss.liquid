      margin-bottom: box-scale(2);
    }

    h1 {
      font-size: text-scale(7);
    }

    h2 {
      font-size: text-scale(7);
      margin-bottom: box-scale(6);

      &:nth-child(n+2) {
        border-top: 1px solid #e0e0e0;
        margin-top: 98px;
        padding-top: 98px;
      }
    }

    h3, h4, h5, h6 {
      font-size: text-scale(0);
      text-transform: uppercase;
      font-weight: 700;
    }

    p + h3, p + h4, p + h5, p + h6, ul + h3, ul + h4, ul + h5, ul + h6, ol + h3, ol + h4, ol + h5, ol + h6 {
      margin-top: box-scale(6);
    }

    ul {
      @extend %theme-green-bullets;
      margin-bottom: text-scale(9);

      li {
        margin-bottom: text-scale(2);
      }
    }

    ol {
      @extend %list-default;
      margin-bottom: text-scale(9);
      counter-reset: list;

      li {
        margin-bottom: text-scale(2);
        list-style: none;

        &:before {
          content: counter(list, decimal) ". ";
          counter-increment: list;
          padding-right: 16px;
          font-weight: bold;
        }

        @include breakpoint(null, $large, null) {
          white-space: nowrap;
        }
      }
    }

    img {
      max-width: 100%;
    }

    abbr[title] {
      border-bottom: 1px dotted;
    }

    .fluid-width-video-wrapper {
      margin-bottom: text-scale(0);
    }
  }
}

